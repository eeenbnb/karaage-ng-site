{
  "name": "karaage-ng-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node src/build/generate-list.js && ng build --prod --aot --vendor-chunk --common-chunk --delete-output-path --build-optimizer && cp CNAME ./docs/CNAME && node src/build/generate-sitemap.js && node src/build/generate-rss.js",
    "build:netlify": "node src/build/generate-list.js && ng build --prod --aot --vendor-chunk --common-chunk --delete-output-path --build-optimizer --stats-json && node src/build/generate-sitemap.js && node src/build/generate-rss.js && cp -r ./storybook-static ./docs/assets/stroybook && cp _redirects ./docs/_redirects",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "scully": "npx scully --",
    "scully:serve": "npx scully serve --",
    "analyzer": "webpack-bundle-analyzer docs-static/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.1",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/forms": "~12.0.1",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/router": "~12.0.1",
    "@capacitor/android": "^3.0.0",
    "@capacitor/cli": "^3.0.0",
    "@capacitor/core": "^3.0.0",
    "@capacitor/ios": "^3.0.0",
    "@ngrx/data": "^12.2.0",
    "@ngrx/effects": "^12.2.0",
    "@ngrx/entity": "^12.2.0",
    "@ngrx/store": "^12.0.0",
    "@ngrx/store-devtools": "^12.0.0",
    "@scullyio/init": "^1.1.4",
    "@scullyio/ng-lib": "^1.0.0",
    "@scullyio/scully": "^1.0.0",
    "firebase-tools": "^9.11.0",
    "github-markdown-css": "^4.0.0",
    "glob": "^7.1.6",
    "glob-promise": "^4.1.0",
    "marked": "^2.0.3",
    "ngx-markdown": "^12.0.1",
    "prismjs": "^1.23.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-markdown": "^6.0.2",
    "rehype-raw": "^5.1.0",
    "rxjs": "~6.6.0",
    "styled-components": "^5.3.0",
    "tslib": "^2.0.0",
    "xml2js": "^0.4.23",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^12.0.0-beta.0",
    "@angular-devkit/build-angular": "~12.0.1",
    "@angular/cli": "~12.0.1",
    "@angular/compiler-cli": "~12.0.1",
    "@babel/core": "^7.13.16",
    "@compodoc/compodoc": "^1.1.11",
    "@ngrx/schematics": "^12.0.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/angular": "^6.2.9",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/styled-components": "^5.1.9",
    "babel-loader": "^8.2.2",
    "codelyzer": "^6.0.0",
    "css-loader": "^5.2.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "sass": "^1.32.13",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4",
    "webpack-bundle-analyzer": "^4.4.1"
  }
}
