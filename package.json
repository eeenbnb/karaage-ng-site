{
  "name": "karaage-ng-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node src/build/generate-list.js && ng build --configuration production --aot --vendor-chunk --common-chunk --delete-output-path --build-optimizer && cp CNAME ./docs/CNAME && node src/build/generate-sitemap.js && node src/build/generate-rss.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "analyzer": "webpack-bundle-analyzer docs-static/stats.json",
    "dev:ssr": "ng run karaage-ng-site:serve-ssr",
    "serve:ssr": "node dist/karaage-ng-site/server/main.js",
    "build:ssr": "ng build && ng run karaage-ng-site:server",
    "prerender": "ng run karaage-ng-site:prerender",
    "prerender:route": "node src/build/generate-list.js && node src/build/generate-route-txt.js && ng run karaage-ng-site:prerender --routes-file route.txt && node src/build/generate-sitemap.js && node src/build/generate-rss.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.1",
