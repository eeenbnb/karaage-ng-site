{
  "name": "karaage-ng-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node src/build/generate-list.js && ng build --prod --aot --vendor-chunk --common-chunk --delete-output-path --build-optimizer && cp CNAME ./docs/CNAME && node src/build/generate-sitemap.js",
    "build:netlify": "node src/build/generate-list.js && ng build --prod --aot --vendor-chunk --common-chunk --delete-output-path --build-optimizer && node src/build/generate-sitemap.js && cp -r ./storybook-static ./docs/assets/stroybook && cp _redirects ./docs/_redirects",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.9",
    "@angular/common": "~11.2.9",
    "@angular/compiler": "~11.2.9",
    "@angular/core": "~11.2.9",
    "@angular/forms": "~11.2.9",
    "@angular/platform-browser": "~11.2.9",
    "@angular/platform-browser-dynamic": "~11.2.9",
    "@angular/router": "~11.2.9",
    "github-markdown-css": "^4.0.0",
    "glob": "^7.1.6",
    "glob-promise": "^4.1.0",
    "marked": "^2.0.3",
    "ngx-markdown": "^11.1.3",
    "prismjs": "^1.23.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "xml2js": "^0.4.23",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.8",
    "@angular/cli": "~11.2.8",
    "@angular/compiler-cli": "~11.2.9",
    "@babel/core": "^7.13.16",
    "@compodoc/compodoc": "^1.1.11",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/angular": "^6.2.9",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.2.2",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
